# This is an example of the .env file
#
# Use it as a template for creating your own .env file
#

GROQ_API_KEY=<YOUR GROQ API KEY>
GROQ_MODEL=<YOUR GROQ MODEL>

HF_TOKEN=<YOUR HF TOKEN>
HF_MODEL=<YOUR HF MODEL>

# ----------------------------------------------------------------------------
# EXTERNAL IMAGE SOURCES (Stock photo APIs)
# ----------------------------------------------------------------------------
# Unsplash API access key for high-quality stock photos
# Get your key from: https://unsplash.com/developers
UNSPLASH_ACCESS_KEY=<YOUR_UNSPLASH_ACCESS_KEY>

# Pexels API key for stock photo search
# Get your key from: https://www.pexels.com/api/
PEXELS_API_KEY=<YOUR_PEXELS_API_KEY>

GEMINI_API_KEY=<YOUR GEMINI API KEY>
GEMINI_MODEL=<YOUR GEMINI MODEL>

REPLICATE_API_TOKEN=<YOUR REPLICATE API TOKEN>
REPLICATE_MODEL=<YOUR REPLICATE MODEL>

LANGCHAIN_TRACING_V2=<TRUE OR FALSE>
LANGCHAIN_API_KEY=<YOUR LANGCHAIN_API_KEY>
LANGCHAIN_PROJECT=<YOUR APP NAME OR SIMILAR>
LANGCHAIN_ENDPOINT=<LANGCHAIN VALID ENDPOINT>

# ----------------------------------------------------------------------------
# FINANCIAL NEWS CONFIGURATION (Alpha Vantage)
# ----------------------------------------------------------------------------
# Alpha Vantage API key for real-time financial news
# Get your key from: https://www.alphavantage.co/support/#api-key
FINANCE_ALPHA_VANTAGE_KEY=<YOUR_ALPHA_VANTAGE_API_KEY>
FINANCE_MAX_ARTICLES=5